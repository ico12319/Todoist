{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ui5.walkthrough",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [""],
      "fallbackLocale": ""
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "todoRemote": {
        "uri": "V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.walkthrough.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      },
      "todo": {
        "dataSource": "todoRemote"
      }
    },
    "rootView": {
      "viewName": "ui5.walkthrough.view.App",
      "type": "XML",
      "id": "app"
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "ui5.walkthrough.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "overview",
          "target": "overview"
        },
        {
          "pattern": "login",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "list/{list_id}",
          "name": "list",
          "target": "list"
        },
        {
          "pattern": "detail/{todosPath}",
          "name": "detail",
          "target": "detail"
        },
        {
          "pattern": "detail-todolist/{todoListPath}",
          "name": "detail-todolist",
          "target": "detail-todolist"
        },
        {
          "pattern": "list/{list_id}/collaborators",
          "name": "collaborators",
          "target": "collaborators"
        }
      ],
      "targets": {
        "login": {
          "id": "login",
          "name": "Login"
        },
        "overview": {
          "id": "overview",
          "name": "Overview"
        },
        "detail-todolist": {
          "id": "detail-todolist",
          "name": "Detail-todoList"
        },
        "detail": {
          "id": "detail",
          "name": "Detail"
        },
        "list": {
          "id": "list",
          "name": "List"
        },
        "collaborators": {
          "id": "collaborators",
          "name": "Collaborators"
        }
      }
    }
  }
}
